    "test": "mocha",
    "test-mode": "mocha --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prep": "yarn run lint && yarn run compile && yarn test",
    "vscode:prepublish": "yarn run compile",
    "vscode:postpublish": "./tag-release.sh"
